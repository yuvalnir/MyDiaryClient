<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="il.ac.hit.mvc.utils.Event" table="EVENTS">
		<id name="id" type="int" column="ID" >
			<generator class="increment"/>
		</id>
		<property name="name">
			<column name="NAME"/>
		</property>
		<property name="price">
			<column name="PRICE"/>
		</property>
	</class>
</hibernate-mapping>