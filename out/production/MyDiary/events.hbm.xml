<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="il.ac.hit.mvc.utils.Event" table="EVENTS">
		<id name="id" type="long" column="ID">
			<generator class="increment"/>
		</id>
		<property name="email">
			<column name="EMAIL"/>
		</property>
		<property name="title">
			<column name="TITLE"/>
		</property>
		<property name="location">
			<column name="LOCATION"/>
		</property>
		<property name="starts">
			<column name="STARTS"/>
		</property>
		<property name="ends">
			<column name="ENDS"/>
		</property>
		<property name="date">
			<column name="DATE"/>
		</property>
		<property name="note">
			<column name="NOTE"/>
		</property>
	</class>
</hibernate-mapping>